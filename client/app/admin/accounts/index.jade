.container
  ul.list-group
    li.list-group-item(ng-repeat='user in users | filter: { role: "!admin" } ')
      strong {{user.name}}
      br
      span.text-muted {{user.email}}
      a.trash(ng-click='delete(user)')
        span.glyphicon.glyphicon-trash.pull-right
